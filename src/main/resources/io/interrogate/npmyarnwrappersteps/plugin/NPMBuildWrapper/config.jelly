<j:jelly xmlns:j="jelly:core" xmlns:c="/lib/credentials" xmlns:f="/lib/form">
  <f:entry field="credentialsId" title="${%NPM Registry Credentials}">
    <c:select />
  </f:entry>
  <f:entry title="${%Workspace Subdirectory (containing .nvmrc)}" field="workspaceSubdirectory">
    <f:textbox />
  </f:entry>
  <f:entry title="${%Enable yarn for this build.}" field="yarnEnabled">
    <f:checkbox/>
  </f:entry>
</j:jelly>
